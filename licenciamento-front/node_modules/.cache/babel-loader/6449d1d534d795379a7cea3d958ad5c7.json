A,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,KALA;AAMA,IAAA,OANA,mBAMA,eANA,EAMA;AAAA;;AACA,UAAA,OAAA,CAAA,yBAAA,eAAA,CAAA,WAAA,GAAA,GAAA,CAAA,EAAA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,eAAA,EACA,IADA,CACA,UAAA,IAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,UAAA,MAAA,CAAA,MAAA;AACA,SAJA;AAKA;AACA,KAdA;AAeA,IAAA,YAfA,wBAeA,WAfA,EAeA;AACA,WAAA,WAAA,GAAA,WAAA;AACA;AAjBA;AAbA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <menu-principal></menu-principal>\r\n\r\n    <pagina-base subtitulo=\"» Listagem de Produtos\">\r\n      <hr />\r\n        <h5><router-link to=\"/Cadastro/Produto\"><img src=\"/img/incluir.png\">Adicionar Produto</router-link></h5>\r\n      <br>\r\n      <jw-pagination\r\n        :items=\"produtos\"\r\n        @changePage=\"onChangePage\"\r\n        :styles=\"customStyles\"\r\n        :labels=\"customLabels\"\r\n      ></jw-pagination>\r\n\r\n      <table class=\"table table-striped\">\r\n        <thead>\r\n          <tr>\r\n            <th>Código</th>\r\n            <th>Nome</th>\r\n            <th>Versão</th>\r\n            <th>Ativo</th>\r\n            <th>Opções</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <!-- <tr v-for=\"s in setores\" :key=\"s.idSetor\">-->\r\n          <tr v-for=\"produto in pageOfItems\" :key=\"produto.idProduto\">\r\n            <td>{{ produto.codigoProduto }}</td>\r\n            <td><router-link :to=\"`/Detalhe/Produto/${produto.idProduto}`\">{{ produto.nomeProduto }}</router-link></td>\r\n            <td>{{ produto.versaoProduto }}</td>\r\n            <td v-if=\"produto.ativo == true\">Sim</td>\r\n            <td v-else>Não</td>\r\n            <td>\r\n              <router-link :to=\"`/Editar/Produto/${produto.idProduto}`\"><div class=\"editar\"></div></router-link>\r\n              <div class=\"excluir\" v-on:click=\"excluir(produto)\"></div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </pagina-base>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport dadosproduto from \"@/services/dadosproduto.js\";\r\n\r\nconst customStyles = {\r\n  li: {\r\n    display: \"inline-block\",\r\n    padding: 0,\r\n    margin: 0,\r\n  },\r\n  a: {\r\n    cursor: \"pointer\",\r\n    padding: \"6px 12px\",\r\n    display: \"block\",\r\n    float: \"left\",\r\n    color: \"black\",\r\n\r\n    textdecoration: \"none\",\r\n  },\r\n};\r\n\r\nconst customLabels = {\r\n  first: \"«\",\r\n  last: \"»\",\r\n  previous: \"<\",\r\n  next: \">\",\r\n};\r\n\r\nexport default {\r\n  name: \"Produto\",\r\n  data() {\r\n    return {\r\n      produtos: [],\r\n      pageOfItems: [],\r\n      customStyles,\r\n      customLabels,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.listar();\r\n  },\r\n  methods: {\r\n    listar() {\r\n      dadosproduto.listar().then((resp) => {\r\n        this.produtos = resp.data;\r\n      });\r\n    },\r\n    excluir(produtoExclusao){\r\n      if(confirm(\"Deseja excluir o(a) \" + produtoExclusao.nomeProduto + \"?\")){\r\n        dadosproduto.excluir(produtoExclusao)\r\n        .then(resp=> {\r\n          console.log(resp);\r\n          this.listar()\r\n        })\r\n      }\r\n    },\r\n    onChangePage(pageOfItems) {\r\n      this.pageOfItems = pageOfItems;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/views/produto"}]}